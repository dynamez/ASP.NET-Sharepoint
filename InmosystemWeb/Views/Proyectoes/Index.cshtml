@model IEnumerable<InmosystemWeb.Models.Proyecto>

@{
    ViewBag.Title = "Proyectos";
}

<h2>Proyectos</h2>

<p>
    @Html.ActionLink("Crear nuevo Proyecto", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Nombre
        </th>
        <th>
            Zona
        </th>
        <th>
            Region
        </th>
        <th>
            Ciudad
        </th>
        <th>
            Calle
        </th>
        <th>
            Gerente
        </th>
        <th>
            Descripcion
        </th>
        <th>
            Codigo Contable
        </th>
        <th>
            Inmobiliaria
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Proy_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Proy_zona)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Proy_region)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Proy_ciudad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Proy_calle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Proy_gerente)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Proy_descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Proy_codigoContable)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Inmobiliaria.Inmo_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Proy_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Proy_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Proy_id })
        </td>
    </tr>
}

</table>
